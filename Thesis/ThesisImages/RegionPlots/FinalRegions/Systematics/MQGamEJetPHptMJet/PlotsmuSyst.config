%    --- %
% --------------- %

Job: "FCNC_All_mujets"
  CmeLabel: "13 TeV"
  POI: "SigXsecOverSM"
  ReadFrom: NTUP
  NtuplePaths: "/scratch/jbarkelo/TRExInputs/mujets/"
  Label: "mu+jets"
  LumiLabel: "139 fb^{-1}"
  MCweight: "event_norm"
  NtupleName: "nominal"
  DebugLevel: 1
  MCstatThreshold: 0.001
  MCstatConstraint: "Poisson"
  SystControlPlots: FALSE
  SystPruningShape: 0.01
  SystPruningNorm: 0.01
  CorrelationThreshold: 0.20
  HistoChecks: NOCRASH
  SplitHistoFiles: TRUE
  ImageFormat: "png"
  SystCategoryTables: TRUE
  RankingPlot: "all"
  RankingMaxNP: 10
  DoSummaryPlot: TRUE
  DoTables: TRUE
  DoSignalRegionsPlot: TRUE
  DoPieChartPlot: TRUE
  AtlasLabel: none %Internal
  TableOptions: FOOTNOTESIZE
  RatioYmax: 1.5
  RatioYmin: 0.5
  PlotOptions: "OVERSIG,NOSIG"
%  YIELDS,OVERSIG,NOSIG

% --------------- %
% ---  FIT    --- %
% --------------- %

Fit: "fit"
  FitType: SPLUSB
%  FitType: BONLY
  FitRegion: CRSR
%  FitRegion: CRONLY
  POIAsimov: 0.
  UseMinos: SigXsecOverSM
%,mu_ttgam,mu_Wgam
  doLHscan: SigXsecOverSM
  LHscanMax: 0.15
  LHscanSteps: 15
%  FitBlind: TRUE

% --------------- %
% ---  LIMIT    --- %
% --------------- %

Limit: "limit"
  LimitType: ASYMPTOTIC
  POIAsimov: .1
%  LimitBlind: TRUE
%  SignalInjection: TRUE

% ---------------- %
% - SIGNIFICANCE - %
% ---------------- %
%Significance: "significance"
%  SignificanceBlind: TRUE
%  POIAsimov: 1.
%  ParamName: "SignalStrength"


% --------------- %
% --- REGIONS --- %
% --------------- %

